{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Common Word IDs",
  "type": "object",
  "required": [
    "schema",
    "ids"
  ],
  "properties": {
    "schema": {
      "type": "object",
      "required": [
        "name",
        "version"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "ids": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  },
  "additionalProperties": true
}
