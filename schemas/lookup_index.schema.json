{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lookup index (exact form â†’ word id list)",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "schema",
    "bucket",
    "map"
  ],
  "properties": {
    "schema": {
      "type": "object",
      "required": [
        "name",
        "version"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      }
    },
    "bucket": {
      "type": "string"
    },
    "map": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      }
    }
  }
}
